{
  "memory": 256,
  "ttl": 10,
  "timeout": 10,
  "minReplicas": 2,
  "maxReplicas": 10,
  "workers": 4,
  "routes": {
    "authorize": {
      "path": "/_v/api/origami-vtex-connector/payments",
      "public": true
    },
    "cancel": {
      "path": "/_v/api/origami-vtex-connector/payments/:paymentId/cancellations",
      "public": true
    },
    "settle": {
      "path": "/_v/api/origami-vtex-connector/payments/:paymentId/settlements",
      "public": true
    },
    "refund": {
      "path": "/_v/api/origami-vtex-connector/payments/:paymentId/refunds",
      "public": true
    },
    "inbound": {
      "path": "/_v/api/origami-vtex-connector/payments/:paymentId/inbound/hooks",
      "public": true
    },
    "eligibility": {
      "path": "/_v/api/origami-vtex-connector/eligibility",
      "public": true
    },
    "confirm": {
      "path": "/_v/api/origami-vtex-connector/confirm",
      "public": true
    }
